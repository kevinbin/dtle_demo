{
    "Name":"mysql3-to-mysql2",
    "Tasks":[
        {
            "Type":"Src",
            "Config":{
                "ReplicateDoDb":[
                    {
                        "TableSchema":"demo",
                        "Tables":[
                            {
                                "TableName":"tbl3",
                                "Where":"a>=3"
                            }
                        ]
                    }
                ],
                "ConnectionConfig":{
                    "Host":"mysql3",
                    "Port":"3306",
                    "User":"root",
                    "Password":"pass"
                }
            }
        },
        {
            "Type":"Dest",
            "Config":{
                "ConnectionConfig":{
                    "Host":"mysql2",
                    "Port":"3306",
                    "User":"root",
                    "Password":"pass"
                }
            }
        }
    ]
}